<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    tools:context="com.example.emobilis.kitchenyetu.Store">

    <ImageButton
        android:id="@+id/veg"
        android:layout_width="178dp"
        android:layout_height="170dp"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/textView10"
        app:srcCompat="@drawable/aisle" />

    <ImageButton
        android:id="@+id/salad"
        android:layout_width="178dp"
        android:layout_height="170dp"
        android:layout_alignTop="@+id/veg"
        android:layout_marginStart="43dp"
        android:layout_toEndOf="@+id/veg"
        app:srcCompat="@drawable/bar" />

    <ImageButton
        android:id="@+id/tubers"
        android:layout_width="178dp"
        android:layout_height="170dp"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/veg"
        android:layout_marginTop="73dp"
        app:srcCompat="@drawable/basket" />

    <ImageButton
        android:id="@+id/oil"
        android:layout_width="178dp"
        android:layout_height="170dp"
        android:layout_alignStart="@+id/salad"
        android:layout_alignTop="@+id/tubers"
        app:srcCompat="@drawable/oil" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="178dp"
        android:layout_height="24dp"
        android:layout_above="@+id/oil"
        android:layout_alignParentStart="true"
        android:text="Tubers" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="178dp"
        android:layout_height="24dp"
        android:layout_above="@+id/oil"
        android:layout_alignParentEnd="true"
        android:text="CookingOil" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="178dp"
        android:layout_height="24dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:text="Vegetables" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="178dp"
        android:layout_height="24dp"
        android:layout_alignParentTop="true"
        android:layout_alignStart="@+id/salad"
        android:text="Salad" />
</RelativeLayout>
